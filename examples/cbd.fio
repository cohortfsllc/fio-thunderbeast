######################################################################
# Example test for the Cohort Block Device
# 
# Runs a 4k random write test agains a CBD via libcbd.
#
# NOTE: Make sure you have either a RBD named 'fio_test' or change 
#       the rbdname parameter.
######################################################################
[global]
ioengine=cbd
volume=cbd
image=test_cbd
invalidate=0	# mandatory
rw=randwrite
bs=4k

[cbd_iodepth2]
iodepth=32
